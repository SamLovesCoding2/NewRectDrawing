package rectorder;

/**
 *
 * @author lupeiyu
 */
public class OffSet
{
    public Windows window;
    public int offSetX;
    public int offSetY;

    public OffSet(Windows window, int offSetX, int offSetY)
    {
        this.window = window;
        this.offSetX = offSetX;
        this.offSetY = offSetY;
    }
    
}
